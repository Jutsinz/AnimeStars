 local Toggle = Tab:CreateToggle({
    Name = "Auto Chi",
    Info = "Spams Chi Upgrade Must TP TO CHI BOOST",
    CurrentValue = false,
    Flag = "Toggle1chichi", 
    Callback = function(Value)
        chiChi = Value
        if chiChi then
            pcall(function ()
                local button = game:GetService'VirtualInputManager'
                while chiChi do
                button:SendKeyEvent(true, "E", false, game)
                wait()
                end
                
            end)
        end
    end,
 })

repeat wait() until game:IsLoaded()

local VirtualUser = game:GetService("VirtualUser")

game:GetService("Players").LocalPlayer.Idled:Connect(
    function()
        VirtualUser:CaptureController()
        VirtualUser:ClickButton2(Vector2.new())
    end
)

local nameOfGame = game:GetService("MarketplaceService"):GetProductInfo(game.PlaceId)

local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/UI-Interface/CustomFIeld/main/RayField.lua'))()

local Window = Rayfield:CreateWindow({
    Name = nameOfGame.Name,
    LoadingTitle = "I LOVE BOOBIES HUB",
    LoadingSubtitle = "by ThatOneGuy",
    ConfigurationSaving = {
       Enabled = true,
       FolderName = nil, 
       FileName = "(.)(.) Hub"
    }
 })

 local Tab = Window:CreateTab("I'm OP NOW")

local Toggle = Tab:CreateToggle({
   Name = "Auto Tap",
   CurrentValue = false,
   Flag = "TapToggle1", 
   Callback = function(Value)
    autotap = Value
    if autotap then
        pcall(function ()
            while autotap do
                for i=1, 10 do
                task.wait()
                local args = {
                    [1] = {
                        [1] = {
                            [1] = "\5",
                            [2] = "Tapping"
                        }
                    }
                }

                game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
                end
            end
        end)
    end
end,
})

local Toggle = Tab:CreateToggle({
    Name = "Auto Super",
    CurrentValue = false,
    Flag = "SuperToggle1", 
    Callback = function(Value)
     autoSuper = Value
     if autoSuper then
         pcall(function ()
             while autoSuper do
                game:GetService("Players").LocalPlayer.PlayerGui.Interface.Guis.Message:Destroy()
                 local args = {

local rebs = game:GetService("Players").LocalPlayer.PlayerGui.Interface.Guis.Rebirth.MainFrame.List

local rebsTable = {}

for i,v in ipairs(rebs:GetChildren()) do
    if v:IsA("Frame") then
        local rebirthNumber = tonumber(string.match(v.Name, "%d+")) 
        table.insert(rebsTable, rebirthNumber)
    end
end

local Dropdown = Tab:CreateDropdown({
    Name = "Select Rebirth Option",
    Options = rebsTable,
    CurrentOption = " ",
    Flag = "selReb1",
    Callback = function(Option)
       _G.selReb1 = Option;
    end,
 })
 
  local Toggle = Tab:CreateToggle({
    Name = "Auto Rebirth",
    CurrentValue = false,
    Flag = "autoReb", 
    Callback = function(Value)
        _G.autoReb = Value;
    end,
 })

task.spawn(function()
    while wait() do
        if _G.autoReb then
            local args = {
                [1] = {
                    [1] = {
                        [1] = "\3",
                        [2] = "Rebirth",
                        [3] = {
                            ["Rebirth"] = _G.selReb1 
                        }
                    }
                }
            }
            
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
            wait()
        end
    end
end)

